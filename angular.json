{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vault-projet": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/vault-projet",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
           "styles": [
  "src/styles.css",
  "src/assets/template/vendor/bootstrap/css/bootstrap.min.css",
  "src/assets/template/vendor/bootstrap-icons/bootstrap-icons.css",
  "src/assets/template/vendor/aos/aos.css",
  "src/assets/template/vendor/glightbox/css/glightbox.min.css",
  "src/assets/template/vendor/swiper/swiper-bundle.min.css",
  "src/assets/template/css/main.css",
  "node_modules/@fortawesome/fontawesome-free/css/all.min.css"
],
            "scripts": [
 "src/assets/template/vendor/bootstrap/js/bootstrap.bundle.min.js",
  "src/assets/template/vendor/glightbox/js/glightbox.min.js",
  "src/assets/template/vendor/swiper/swiper-bundle.min.js",
  "src/assets/template/vendor/imagesloaded/imagesloaded.pkgd.min.js",
  "src/assets/template/vendor/isotope-layout/isotope.pkgd.min.js",
  "src/assets/template/vendor/php-email-form/validate.js",
  "src/assets/template/vendor/waypoints/noframework.waypoints.js",
  "src/assets/template/vendor/aos/aos.js",
  "src/assets/template/js/main.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "vault-projet:build:production"
            },
            "development": {
              "buildTarget": "vault-projet:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
